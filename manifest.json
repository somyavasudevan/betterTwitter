{
    "name": "BrowserExtension",
    "version": "0.0.1",
    "manifest_version": 2,
    "description" : "Tweet analysis",
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "content_scripts": [{
        "matches": [ "https://twitter.com/*"],
        "js": ["jquery.js","content.js"]
         "css": ["styles.css"]
       
    }]

}


